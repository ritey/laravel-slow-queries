{
  "name": "ritey/laravel-slow-queries",
  "description": "Collects slow queries via DB::listen, batches them every 10 minutes, emails and rotates.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Ritey\\SlowQueries\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ritey\\SlowQueries\\SlowQueriesServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=7.2.0",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/console": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/mail": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/filesystem": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  }
}
